	background-image: url(../images/news_vline.gif);
	background-position: 624px 0;
	background-repeat: repeat-y;
}

.left_lr {
	width:315px;
	float: left;
}

/* center left */

.center_cl {
	width:625px;
	padding: 20px 0 0 0;
	float: left;
	background-image: url(../images/news_vline.gif);
	background-position: 0 0;
	background-repeat: repeat-y;
}

.right_cl {
	width:0;
}

.left_cl {
	width:315px;
	padding: 20px 0 0 0;
	float: left;
}

/* center right */

.center_cr {
	width:625px;
	padding: 20px 0 0 0;
	float: left;
	background-image: url(../images/news_vline.gif);
