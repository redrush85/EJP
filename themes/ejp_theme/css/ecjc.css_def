/* Footer
-----------------------------------------------------------------------------*/

#footer_bg{
	background-image: url(../images/footer_bg.jpg);
	background-position: top center;
	background-repeat: no-repeat;
	background-color: #6fbbdd;
	color: #fff!important;
	height: 100%;
	margin-top:30px;
}

#footer {
	width: 970px;
	margin: 0 auto;
	padding-top: 40px;
	text-align: left;
}

/*autocomplete*/
#autocomplete{
	line-height: 20px;
}

#ECJC_search-wrapper .form-autocomplete {
  background-position: 99% 4px;
}

#ECJC_search-wrapper .throbbing {
  background-position: 99% -16px;
}

#ECJC_search-wrapper  #autocomplete li{
	padding: 5px 10px 5px 10px;
}

#ECJC_search-wrapper  #autocomplete {
	width:202px;
	height:auto;
	position:absolute;
	z-index: 15;
	top: 9px;
	border: 1px solid #d2d2d2;
}

#ECJC_search-wrapper {
position:relative;
}

#ECJC_search-wrapper .form-autocomplete  { z-index:20;position:relative; }
#ECJC_search-wrapper  #autocomplete ul li{
color: #505050;
}

#ECJC_search-wrapper  #autocomplete ul li.selected{
background:#247a9d;
color: white;
}

/*autocomplete*/

.news_updates .woman,
.news_updates .man{
  clear: both;
}
.news_updates .woman li,
.news_updates .man li{
  display: inline;
  float: left;
